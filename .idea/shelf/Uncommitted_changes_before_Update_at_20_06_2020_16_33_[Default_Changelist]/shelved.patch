<+>{% extends 'base.html.twig' %}\r\n\r\n{% block title %}Practicant index{% endblock %}\r\n\r\n{% block body %}\r\n    <h1>Practicant index</h1>\r\n\r\n    <table class=\"table\">\r\n        <thead>\r\n            <tr>\r\n                <th>Id</th>\r\n                <th>Username</th>\r\n                <th>Roles</th>\r\n                <th>Password</th>\r\n                <th>FirstName</th>\r\n                <th>LastName</th>\r\n                <th>actions</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n        {% for practicant in practicants %}\r\n            <tr>\r\n                <td>{{ practicant.id }}</td>\r\n                <td>{{ practicant.username }}</td>\r\n                <td>{{ practicant.roles ? practicant.roles|json_encode : '' }}</td>\r\n                <td>{{ practicant.password }}</td>\r\n                <td>{{ practicant.firstName }}</td>\r\n                <td>{{ practicant.lastName }}</td>\r\n                <td>\r\n                    <a href=\"{{ path('practicant_show', {'id': practicant.id}) }}\">show</a>\r\n                    <a href=\"{{ path('practicant_edit', {'id': practicant.id}) }}\">edit</a>\r\n                </td>\r\n            </tr>\r\n        {% else %}\r\n            <tr>\r\n                <td colspan=\"7\">no records found</td>\r\n            </tr>\r\n        {% endfor %}\r\n        </tbody>\r\n    </table>\r\n\r\n    <a href=\"{{ path('practicant_new') }}\">Create new</a>\r\n{% endblock %}\r\n
