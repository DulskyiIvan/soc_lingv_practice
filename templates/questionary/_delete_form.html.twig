<form method="post" action="{{ path('questionary_delete', {'id': questionary.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ questionary.id) }}">
    <button class="btn">Delete</button>
</form>
